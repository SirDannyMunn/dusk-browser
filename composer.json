{
    "name": "tpccdaniel/dusk-secure",
    "description": "Dusk secure is dusk for browser automation only.",
    "require": {
        "php": ">=7.1.0",
        "facebook/webdriver": "~1.3",
        "nesbot/carbon": "~1.20",
        "illuminate/console": "~5.6",
        "illuminate/support": "~5.6",
        "symfony/console": "~4.0",
        "symfony/process": "^4.3@dev"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Tpccdaniel\\DuskSecure\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Tpccdaniel\\DuskSecure\\DuskServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
